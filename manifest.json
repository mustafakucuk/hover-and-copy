{
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "default_locale": "en",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "main.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}